<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hajjhealthcare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo1_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/login" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".VideoCallActivity" />
        <activity android:name=".ServicesActivity" />
        <activity android:name=".BotActivity" />
        <activity android:name=".MedicalAssistanceActivity" />
        <activity android:name=".SuggestionsComplainsActivity" />
        <activity android:name=".VideoCallSummaryActivity" />
        <activity android:name=".VerficationActivity" />
        <activity android:name=".VerficationCodeActivity"></activity>
    </application>

</manifest>